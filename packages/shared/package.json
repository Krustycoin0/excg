{
  "name": "@transferto/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./index.d.ts",
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "hook:build-test": "tsc --build --clean && tsc --project tsconfig.json --noEmit && tsc --build tsconfig.json && echo 'Build succeeded in SHARED package' || echo 'Build failed in SHARED package'",
    "test": "run-s 'test:*'",
    "test:unit": "echo \"No tests yet\"",
    "fix:staged-files": "lint-staged --allow-empty",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@lifi/wallet-management": "^2.5.2",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@ungap/structured-clone": "^1.2.0",
    "i18next": "^23.7.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.4.1",
    "typescript": "^4.5.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.0",
    "@types/ungap__structured-clone": "^0.3.2",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0"
  }
}

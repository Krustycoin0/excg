{
  "name": "@transferto/dapp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@arcxmoney/analytics": "^1.9.1",
    "@cookie3/analytics": "^0.6.0",
    "@lifi/sdk": "^2.5.0",
    "@lifi/wallet-management": "^2.5.2",
    "@lifi/widget": "^2.9.2",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@safe-global/safe-apps-sdk": "^8.1.0",
    "@sentry/browser": "^7.80.1",
    "@sentry/integrations": "^7.80.1",
    "@sentry/react": "^7.80.1",
    "@tanstack/react-query": "^5.8.4",
    "@transferto/shared": "*",
    "@widgetbot/react-embed": "^1.9.0",
    "ethers": "^6.8.0",
    "i18next": "^23.7.6",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.1",
    "i18next-resources-to-backend": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotjar": "^6.1.0",
    "react-i18next": "^13.4.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.18.0",
    "typescript": "^4.5.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@lerna/run": "^6.6.2",
    "@lifi/types": "^9.3.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint-config-prettier": "^9.0.0",
    "fontaine": "^0.4.1",
    "husky": "^8.0.3",
    "lerna": "^7.4.2",
    "prettier": "^3.1.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "vite": "^4.5.0",
    "web-vitals": "^3.5.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode localhost",
    "dev:testnet": "vite --mode testnet",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "hook:prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "hook:build-check": "tsc --build --clean && tsc --project tsconfig.json --noEmit && tsc --build tsconfig.json && echo 'Build succeeded in DAPP package' || echo 'Build failed in DAPP package'",
    "hook:linter": "eslint --fix src && echo /'ESLint: Files have been linted in DAPP package.'/ || echo /'ESLint: No files needed to be linted in DAPP package.'/",
    "fix:staged-files": "lint-staged --allow-empty",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "build:testnet": "tsc && vite build --mode testnet",
    "build:develop": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build --mode production"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
